## do not change this value 
subdir=examples/cpp/heteroAST


srcdir = /d/MyProject/MGPLib/3rdparty/antlr-2.7.7/examples/cpp/heteroAST/../../../examples/cpp/heteroAST
objdir = /d/MyProject/MGPLib/3rdparty/antlr-2.7.7/examples/cpp/heteroAST

this : test
test : test1 

all: heteroAST test

g_FILES = \
	$(srcdir)/calc.g \
	$(eol)

g_cxx_FILES = \
  $(objdir)/CalcLexer.cpp \
	$(objdir)/CalcParser.cpp \
  $(eol)

g_hpp_FILES = \
	$(objdir)/CalcLexer.hpp \
  $(objdir)/CalcParser.hpp \
  $(objdir)/CalcParserTokenTypes.hpp \
  $(eol)

g_txt_FILES = \
	$(objdir)/CalcParserTokenTypes.txt \
	$(eol)

g_gen_FILES = \
	$(g_cxx_FILES) \
	$(g_hpp_FILES) \
	$(g_txt_FILES) \
	$(eol)

g_obj_FILES = \
	$(objdir)/CalcLexer.o \
	$(objdir)/CalcParser.o \
  $(eol)

## Source files
heteroAST_cxx_FILES = \
	$(g_cxx_FILES) \
	$(srcdir)/Main.cpp  \
	$(eol)

## Object files
heteroAST_obj_FILES = \
	$(g_obj_FILES) \
	$(objdir)/Main.o  \
	$(eol)

## Test commands ..
test1_cmd = $(objdir)/heteroAST < $(srcdir)/test.in

## Tests to be executed
test1 : heteroAST
	@ v="$(verbose)"; \
    if test $$v -gt 0 ; then \
      /bin/echo "$(test1_cmd)" ; \
    else \
      /bin/echo "exec test1 .."; \
    fi
	@ $(test1_cmd)

## How to make binaries
heteroAST : $(heteroAST_obj_FILES) /d/MyProject/MGPLib/3rdparty/antlr-2.7.7/lib/cpp/src/libantlr.a
	@ /bin/sh /d/MyProject/MGPLib/3rdparty/antlr-2.7.7/scripts/link.sh $@ $(heteroAST_obj_FILES)

## How to compile ANTLR grammar files
$(g_gen_FILES) : $(g_FILES) /d/MyProject/MGPLib/3rdparty/antlr-2.7.7/antlr/antlr.jar
	/bin/sh /d/MyProject/MGPLib/3rdparty/antlr-2.7.7/scripts/antlr.sh  $(g_FILES)

## GNU make - how to make object file
$(objdir)/%.o : $(srcdir)/%.cpp
	@ CXXINCLUDE="$(srcdir)" /bin/sh /d/MyProject/MGPLib/3rdparty/antlr-2.7.7/scripts/cxx.sh $<

## GNU make - how to make object file
$(objdir)/%.o : $(objdir)/%.cpp
	@ CXXINCLUDE="$(srcdir)" /bin/sh /d/MyProject/MGPLib/3rdparty/antlr-2.7.7/scripts/cxx.sh $<

## GNU make - how to make object file
%.o : $(srcdir)/%.cpp
	@ CXXINCLUDE="$(srcdir)" /bin/sh /d/MyProject/MGPLib/3rdparty/antlr-2.7.7/scripts/cxx.sh $<

## GNU make - how to make object file
%.o : $(objdir)/%.cpp
	@ CXXINCLUDE="$(srcdir)" /bin/sh /d/MyProject/MGPLib/3rdparty/antlr-2.7.7/scripts/cxx.sh $<

## Housecleaning
clean:
	/usr/bin/rm.exe -r -f *.o *.obj *.exe heteroAST  $(g_gen_FILES) test 

distclean: clean
	/usr/bin/rm.exe -r -f Makefile

# Dependencies
