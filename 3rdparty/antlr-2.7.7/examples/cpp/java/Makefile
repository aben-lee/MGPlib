## Housecleaning
clean:
	/usr/bin/rm.exe -r -f *.o *.obj *.exe java \
		$(java_g_gen_FILES) $(java_tree_g_gen_FILES) \
		$(objdir)/.java.tree.g $(objdir)/.java.g

distclean: clean
	/usr/bin/rm.exe -r -f Makefile

# Dependencies

# the tree parser depends on the parser/lexer
# these help files are necessary to prevent loops
$(objdir)/.java.tree.g: $(objdir)/.java.g

$(java_tree_g_gen_FILES): $(objdir)/.java.tree.g
$(java_g_gen_FILES): $(objdir)/.java.g

$(objdir)/JavaLexer.o: $(objdir)/JavaLexer.hpp $(objdir)/JavaTokenTypes.hpp
$(objdir)/JavaRecognizer.o: $(objdir)/JavaRecognizer.hpp $(objdir)/JavaTokenTypes.hpp
$(objdir)/JavaTreeParser.o: $(objdir)/JavaTreeParser.hpp $(objdir)/JavaTreeParserTokenTypes.hpp
$(objdir)/Main.o: $(objdir)/JavaRecognizer.hpp $(objdir)/JavaTokenTypes.hpp
$(objdir)/Main.o: $(objdir)/JavaTreeParser.hpp $(objdir)/JavaTreeParserTokenTypes.hpp

## If cxx.sh script changed ..
$(java_obj_FILES) : /d/MyProject/MGPLib/3rdparty/antlr-2.7.7/scripts/cxx.sh

## If link.sh script changed ..
$(java_obj_FILES) : /d/MyProject/MGPLib/3rdparty/antlr-2.7.7/scripts/link.sh

