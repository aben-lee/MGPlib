## Test commands ..
test1_cmd = $(objdir)/java $(srcdir)/tests/T.java
test2_cmd = $(objdir)/java $(srcdir)/tests/E.java

## Tests to be executed
test1 : java
	@ v="$(verbose)"; \
    if test $$v -gt 0 ; then \
      /bin/echo "$(test1_cmd)" ; \
    else \
      /bin/echo "exec test1 .."; \
    fi
	@ $(test1_cmd)

## How to make binaries
java : $(java_obj_FILES) /d/MyProject/MGPLib/3rdparty/antlr-2.7.7/lib/cpp/src/libantlr.a
	@ /bin/sh /d/MyProject/MGPLib/3rdparty/antlr-2.7.7/scripts/link.sh $@ $(java_obj_FILES)

## How to compile ANTLR grammar files
$(objdir)/.java.g : $(java_g_FILES) /d/MyProject/MGPLib/3rdparty/antlr-2.7.7/antlr/antlr.jar
	/bin/sh /d/MyProject/MGPLib/3rdparty/antlr-2.7.7/scripts/antlr.sh  $(java_g_FILES)
	$(TOUCH) `/usr/bin/cygpath.exe -w $(objdir)/.java.g`

$(objdir)/.java.tree.g : $(java_tree_g_FILES) /d/MyProject/MGPLib/3rdparty/antlr-2.7.7/antlr/antlr.jar
	/bin/sh /d/MyProject/MGPLib/3rdparty/antlr-2.7.7/scripts/antlr.sh  $(java_tree_g_FILES)
	$(TOUCH) `/usr/bin/cygpath.exe -w $(objdir)/.java.tree.g`

